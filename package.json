  "scripts": {
    "dev": "vite --port 3001",
    "dev:electron": "cross-env ELECTRON_DEV_PORT=3014 concurrently \"vite --port 3014 --strictPort\" \"wait-on http://localhost:3014 && cross-env NODE_ENV=development electron .\"",
    "dev:electron:safe": "cross-env ELECTRON_DEV_PORT=3015 concurrently \"vite --port 3015 --strictPort\" \"wait-on http://localhost:3015 && cross-env NODE_ENV=development electron .\"",
    "dev:electron:auto": "node -e \"const config = require('./dev.config.js'); const port = config.DEFAULT_DEV_PORT; process.env.ELECTRON_DEV_PORT = port; require('child_process').spawn('npm', ['run', 'dev:electron'], {stdio: 'inherit', shell: true});\"",
    "dev:electron:smart": "node smart-electron-launcher.js",
    "dev:electron:smart:win": "node smart-electron-launcher-winfix.js",
    "dev:electron:simple": "node simple-electron-launcher.js",
    "dev:port-check": "node -e \"const config = require('./dev.config.js'); config.isPortAvailable(3014).then(available => console.log('Port 3014 available:', available));\"",
    "build": "vite build",
    "preview": "vite preview --strictPort --port 4173",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e:desktop": "cross-env E2E_TEST=1 PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=1 playwright test --config=playwright.desktop.config.ts",
    "build:electron": "vite build && electron-builder",
    "electron": "electron .",
    "clean-install": "rm -rf node_modules package-lock.json && npm install",
    "reset-project": "npm run clean-install && npm run build",
    "full-system": "powershell -ExecutionPolicy Bypass -File run-system.ps1",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "backup:export": "tsx scripts/export-backups.ts",
    "security:check": "tsx scripts/check-security-updates.ts",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  }
}
